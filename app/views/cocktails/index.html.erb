<div class="container">
  <div class="index">
  <h1>Cocktails</h1>
  <ul>
    <% @cocktails.each do |c| %>
    <li><%= link_to c.name, cocktail_path(c.id) %>
    </li>
    <% end %>
  </ul>
</div>
<div class="call-to-action">
<%= link_to "Create your own Cocktail", new_cocktail_path, class: "btn btn-success" %>
</div>
</div>
